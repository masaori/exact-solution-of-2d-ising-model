    #note[
      $
        mat(
          psi_mu^dagger,
          psi_mu
        )
        :=
        (
          (
            plus
            sqrt(
              -1
            )
            sqrt(
              gamma_2(theta_(mu))
              gamma_2(-theta_(mu))
            )
          )
          hat(Z)_mu^((minus))
          +
          (
            gamma_2(-theta_(mu))
          )
          hat(Y)_mu
          ,
          (
            minus
            sqrt(
              -1
            )
            sqrt(
              gamma_2(theta_(mu))
              gamma_2(-theta_(mu))
            )
          )
          hat(Z)_mu^((minus))
          +
          (
            gamma_2(-theta_(mu))
          )
          hat(Y)_mu
        )
      $

      $
        [hat(Z)_mu^((plus.minus)), hat(Z)_nu^((plus.minus))]_(+) = 2M delta^M_(mu + nu, 0) I
        quad ("複合同順")
        \
        [hat(Z)_mu^((plus.minus)), hat(Z)_nu^((minus.plus))]_(+) = underbrace(
            2M delta^M_(mu + nu, 0)
            I_((CC^2)^(times.o M))
            ,
            [hat(Z)_mu^((plus.minus)), hat(Z)_nu^((plus.minus))]_(+)
            )
            +
            (
              -2
              exp(
                -
                sqrt(-1)
                (2 pi) / M
                (
                  mu
                  +
                  nu
                )
              )
              dot.op
              2I_((CC^2)^(times.o M))
            )
        quad ("複合同順")
        \
        [hat(Z)_mu^((plus.minus)), hat(Y)_nu]_(+) = 0
        \
        [hat(Y)_mu, hat(Y)_nu]_(+) = 2M delta^M_(mu + nu, 0) I
      $

    ]
  ]
]
