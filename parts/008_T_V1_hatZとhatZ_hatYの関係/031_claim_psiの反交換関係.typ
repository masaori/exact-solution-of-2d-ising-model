      dot.op
      (
        2M delta^M_(mu + nu, 0) I_((CC^2)^(times.o M))
      )
      +
      (
        gamma_2(-theta_(mu))
      )
      (
        gamma_2(-theta_(nu))
      )
      dot.op
      (
        2M delta^M_(mu + nu, 0) I_((CC^2)^(times.o M))
      )
      \
